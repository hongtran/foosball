<div class="container">
	<table class="table">
		<thead class="thead thead-default">
			<tr>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Edit</th>
				
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
			<tr>
				<td><%= user.first_name %></td>
				<td><%= user.last_name %></td>
				<td><%= link_to "Edit", edit_user_path(user), class: "btn btn-secondary" %></td>
				
			</tr>
			<% end %>
		</tbody>	
	</table>
</div>